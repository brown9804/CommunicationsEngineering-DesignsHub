# Making python makefile because we need to
# delete the result photos


#******************************************************************************
############  		Modify this part with your own
############ 			files/folders paths.
#******************************************************************************
SRC = ./src/
RESULTS = ./results/



#******************************************************************************
############ 						Run python script
#******************************************************************************
all: binary_channel

binary_channel:
	python3 $(SRC)main.py

#******************************************************************************
############ 							CLEAN FOR ALL
#******************************************************************************


.PHONY: clean
clean:
	rm -rf $(RESULTS)*.jpg
